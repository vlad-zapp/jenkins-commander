{
  "manifest_version": 3,
  "name": "Jenkins Commander",
  "description": "Toolset for jenkins to make your life easier",
  "version": "0.1",
  "permissions": [
    "storage", "scripting", "tabs"
  ],
  "host_permissions": [
    "http://*/*", "https://*/*"
  ],
  "background": {
    "service_worker": "src/bg.js"
  },
  "action": {
    "default_title": "Jenkins Commander"
  }
}