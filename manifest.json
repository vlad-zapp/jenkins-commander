{
  "manifest_version": 3,
  "name": "Jenkins Commander",
  "description": "Toolset for jenkins to make your life easier",
  "version": "0.1",
  "permissions": [
    "storage", "scripting", "tabs", "cookies"
  ],
  "host_permissions": [
    "http://*/*", "https://*/*", "http://localhost:*/*"
  ],
  "background": {
    "service_worker": "src/bg.js"
  },
  "action": {
    "default_title": "Jenkins Commander"
  },
  "icons": {
    "48": "media/icon-off-32.png"
  },
  "web_accessible_resources": [{
    "matches": ["<all_urls>"],
    "resources": ["media/icon-menu.svg"]
  }]
}